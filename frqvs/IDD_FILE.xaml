<Window x:Class="frqvs.IDD_FILE"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:frqvs"
        mc:Ignorable="d"
        Title="Имя файла" Height="157.627" Width="300">
    <Grid>
        <Label x:Name="label" Content="Введите, пожалуйста, имя файла" HorizontalAlignment="Left" Height="24" Margin="10,10,0,0" VerticalAlignment="Top" Width="272" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Padding="0"/>
        <TextBox x:Name="IDC_FILE" HorizontalAlignment="Left" Height="22" Margin="10,39,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272"/>
        <Button x:Name="IDC_FILEOK_BUTTON" Content="ОК" HorizontalAlignment="Left" Height="20" Margin="102,86,0,0" VerticalAlignment="Top" Width="87" Background="{x:Null}" Click="IDC_FILEOK_BUTTON_Click"/>

    </Grid>
</Window>
